CAAA,EAAA,OAAA,EAAA,SAAA,CAAA,CAAA,GAAA,KAAA,eAAA,CAAA,CAAA,EAAA,QAAA,EAAA,CAAA,EAAA,MAAA,CAAA;AACA;;AAEA,eAAA,cAAA,CAAA,KAAA,eAAA,CAAA,CAAA,EAAA,KAAA,CAAA,OAAA,IAAA,WAAA;AACA,SAPA,MAOA;AACA,eAAA,cAAA,CAAA,KAAA,eAAA,CAAA,CAAA,EAAA,KAAA,CAAA,OAAA,IAAA,KAAA,eAAA,CAAA,CAAA,EAAA,IAAA,IAAA,EAAA;AACA;AACA;;AACA,WAAA,UAAA,GAAA,WAAA;AACA,WAAA,YAAA,GAAA,IAAA,CAAA,SAAA,CAAA,KAAA,cAAA,CAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,aAAA,IAAA;AACA,KA/FA;;AAgGA,IAAA,cAAA,CAAA,IAAA,EAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,aAAA,IAAA;AACA,KAnGA;;AAoGA,IAAA,UAAA,GAAA;AACA,WAAA,eAAA,GAAA,EAAA;AACA,WAAA,eAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,kBAAA,CAAA,CAAA,CAFA,CAGA;AACA,KAxGA;;AAyGA,IAAA,QAAA,CAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,uBAAA;;AACA,UAAA,KAAA,CAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,iBAAA,EAAA,KAAA,CAAA,KAAA;AACA,aAAA,eAAA,CAAA,KAAA,CAAA,KAAA,EAAA,MAAA,IAAA,KAAA,CAAA,IAAA;AACA,OAHA,MAGA;AACA,aAAA,eAAA,CAAA,KAAA,CAAA,KAAA,EAAA,MAAA,IAAA,EAAA;AACA;AACA,KAjHA;;AAkHA,IAAA,UAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,GAAA,IAAA,CAAA,KAAA,CAAA,GAAA,CAAA;;AACA,UAAA,MAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,aAAA,IAAA,CAAA,IAAA,MAAA,EAAA;AACA,cAAA,CAAA,GAAA,CAAA,EAAA;AACA,YAAA,MAAA,CAAA,CAAA,CAAA,GAAA,MAAA,CAAA,CAAA,CAAA,CAAA,SAAA,CAAA,CAAA,EAAA,CAAA,EAAA,WAAA,KAAA,MAAA,CAAA,CAAA,CAAA,CAAA,SAAA,CAAA,CAAA,EAAA,MAAA,CAAA,CAAA,CAAA,CAAA,MAAA,CAAA;AACA;AACA;;AACA,QAAA,MAAA,CAAA,CAAA,CAAA,GAAA,MAAA,CAAA,CAAA,CAAA,CAAA,SAAA,CAAA,CAAA,EAAA,CAAA,EAAA,WAAA,KAAA,MAAA,CAAA,CAAA,CAAA,CAAA,SAAA,CAAA,CAAA,EAAA,MAAA,CAAA,CAAA,CAAA,CAAA,MAAA,CAAA;AACA,QAAA,IAAA,GAAA,MAAA,CAAA,IAAA,CAAA,EAAA,CAAA;AACA,QAAA,IAAA,GAAA,IAAA,CAAA,OAAA,CAAA,MAAA,EAAA,EAAA,CAAA;AACA,OATA,MASA;AACA,QAAA,IAAA,GAAA,IAAA,CAAA,SAAA,CAAA,CAAA,EAAA,CAAA,EAAA,WAAA,KAAA,IAAA,CAAA,SAAA,CAAA,CAAA,EAAA,IAAA,CAAA,MAAA,CAAA;AACA,QAAA,IAAA,GAAA,IAAA,CAAA,OAAA,CAAA,MAAA,EAAA,EAAA,CAAA;AACA;;AAEA,aAAA,IAAA;AAEA;;AApIA,GApDA;AA0LA,EAAA,QAAA,EAAA,EA1LA;AA6LA,EAAA,KAAA,EAAA;AA7LA,CAAA","sourcesContent":["<script>\r\nimport DatePickerCustom from \"@/components/DatePicker.vue\";\r\nimport Modal from \"@/components/Modal.vue\";\r\nimport ApiService from \"@/mixins/apiService\";\r\nimport UploadImage from \"@/components/UploadImage.vue\";\r\nexport default {\r\n  name: \"formSample\",\r\n  components: {\r\n    DatePickerCustom,\r\n    Modal,\r\n    UploadImage\r\n  },\r\n  mixins:[ApiService],\r\n  data: function(){\r\n      return{\r\n          name: this.$route.query.name,\r\n          currentRadioOption: {},\r\n          currentCheckboxOption: {},\r\n          inUseFormSample:[],\r\n          submitFormData: {},\r\n          showJsonModal: false,\r\n          modalTitle: \"\",\r\n          modalContent: \"\",\r\n          formNotFound: false\r\n      } \r\n  },\r\n  created: function(){\r\n      let formatName = this.splitInput(this.name);\r\n      let getParseFormSample = this.$store.state['formBuildJson'];\r\n      \r\n      if(getParseFormSample){\r\n            this.showForm(JSON.parse(getParseFormSample),formatName);\r\n      }else{\r\n            var loadApi = this.loadFormBuilder(\"tester\");\r\n            loadApi.then(res=>{\r\n                if(res.status==200){\r\n                    if(res.data.returnCode!==\"MO00201\"){\r\n                        if(res.data.formList!=\"\"){\r\n                            getParseFormSample = JSON.parse(res.data.formList);\r\n                            this.$store.commit(\"formBuildJson\", res.data.formList);\r\n                            this.showForm(getParseFormSample,formatName);\r\n                        }else{\r\n                            this.formSample = null;\r\n                            this.formNotFound = true;\r\n                        }\r\n                    }else{\r\n                        this.formSample = null;\r\n                        this.formNotFound = true;\r\n                    }\r\n                }else{\r\n                    this.formSample = null;\r\n                    this.formNotFound = true;\r\n                    alert(\"Server error.\");\r\n                }\r\n            });\r\n      }\r\n  },\r\n  methods: {\r\n        showForm(json,name){\r\n            let getField = json[\"pages\"].filter((node) => node.formName === name)[0] || \"\";\r\n\r\n            if(getField!=\"\"){\r\n                this.formNotFound = false;\r\n                this.formSample = getField;\r\n                console.log(\"running formSample\",this.formSample);\r\n\r\n                this.inUseFormSample = this.formSample['elements'];\r\n\r\n                this.$store.commit(\"formInUseDefault\", JSON.stringify(this.inUseFormSample));\r\n            }else{\r\n                this.formSample = null;\r\n                this.formNotFound = true;\r\n            }\r\n        },\r\n        getPlaceholder(bool){ //in component maybe\r\n            if(bool==true){\r\n                return \"(Required)\";\r\n            }else{\r\n                return \"\";\r\n            }\r\n        },\r\n        checkRadioClick(indexRadio,id){\r\n            this.currentRadioOption = {};\r\n            this.currentRadioOption = this.inUseFormSample.filter((node) => node.id === id)[0];\r\n            var filterOption =  this.currentRadioOption.options;\r\n\r\n            for(var i in filterOption){\r\n                filterOption[i]['checked'] = false;\r\n            }\r\n            filterOption[indexRadio]['checked'] = true;\r\n            this.currentRadioOption.options = filterOption;\r\n            this.inUseFormSample.find(v => v.id == id).options = this.currentRadioOption.options;\r\n            console.log(\"show\",this.inUseFormSample);\r\n\r\n        },\r\n        checkMultipleCheckboxClick(indexCheckbox,id){\r\n            this.currentCheckboxOption = {};\r\n            this.currentCheckboxOption = this.inUseFormSample.filter((node) => node.id === id)[0];\r\n            var filterOption =  this.currentCheckboxOption.options;\r\n\r\n            filterOption[indexCheckbox]['checked'] = !filterOption[indexCheckbox]['checked'];\r\n    \r\n            this.currentCheckboxOption.options = filterOption;\r\n            this.inUseFormSample.find(v => v.id == id).options = this.currentCheckboxOption.options;\r\n        },\r\n        submitFormJson(bool){\r\n            for(var i in this.inUseFormSample){\r\n                if(this.inUseFormSample[i].type==\"radio\"){\r\n                    for(var h in this.inUseFormSample[i]['options']){\r\n                        if(this.inUseFormSample[i]['options'][h]['checked']==true){\r\n                            this.submitFormData[this.inUseFormSample[i].label.default] = this.inUseFormSample[i]['options'][h]['value']\r\n                        }\r\n                    }   \r\n                }else if(this.inUseFormSample[i].type==\"mobile\"){\r\n                    this.submitFormData[this.inUseFormSample[i].label.default] = {\r\n                        \"countryCode\": this.inUseFormSample[i]['prefix'][0]['data'],\r\n                        \"number\": this.inUseFormSample[i]['prefix'][1]['data']\r\n                    };\r\n                }else if(this.inUseFormSample[i].type==\"select\"){\r\n                    this.submitFormData[this.inUseFormSample[i].label.default] = this.inUseFormSample[i].data || \"\";\r\n           \r\n                }else if(this.inUseFormSample[i].type==\"checkbox\"){\r\n                    var dataArray = [];\r\n                    if(this.inUseFormSample[i]['options']){\r\n                        for(var j in this.inUseFormSample[i]['options']){\r\n                            if(this.inUseFormSample[i]['options'][j]['checked']==true){\r\n                                dataArray.push(this.inUseFormSample[i]['options'][j]['value']);\r\n                            }\r\n                        }\r\n                        if(dataArray.length>0){\r\n                            this.submitFormData[this.inUseFormSample[i].label.default] = dataArray;\r\n                        }else{\r\n                            this.submitFormData[this.inUseFormSample[i].label.default] = \"\";\r\n                        }\r\n                    }else{\r\n                        this.submitFormData[this.inUseFormSample[i].label.default] = this.inUseFormSample[i].data || \"\";\r\n                    }\r\n                }else if(this.inUseFormSample[i].type==\"paneldynamic\"){\r\n                    var valueObject = {};\r\n                    for(var k in this.inUseFormSample[i]['fields']){\r\n                        valueObject[this.inUseFormSample[i]['fields'][k]['label']['default']] = this.inUseFormSample[i]['fields'][k]['data'];\r\n                    }\r\n\r\n                    this.submitFormData[this.inUseFormSample[i].label.default] = valueObject;\r\n                }else{\r\n                    this.submitFormData[this.inUseFormSample[i].label.default] = this.inUseFormSample[i].data || \"\";\r\n                }\r\n            }\r\n            this.modalTitle = \"Form Json\";\r\n            this.modalContent = JSON.stringify(this.submitFormData);\r\n            this.showJsonModal = true;\r\n            return bool;\r\n        },\r\n        closeJsonModal(bool){\r\n            this.showJsonModal = bool;\r\n            return bool;\r\n        },\r\n        clearField(){\r\n            this.inUseFormSample = {};\r\n            this.inUseFormSample = JSON.parse(this.$store.state['formInUseDefault']);\r\n            //this.inUseFormSample = this.formSample['elements'];\r\n        },\r\n        onChange(image) {\r\n            console.log('New picture selected!')\r\n            if (image.file) {\r\n                console.log('Picture loaded.',image.index)\r\n                this.inUseFormSample[image.index][\"data\"] = image.name;\r\n            }else{\r\n                this.inUseFormSample[image.index][\"data\"] = \"\";\r\n            }\r\n        },\r\n        splitInput(name){\r\n          var sArray = name.split(\" \");\r\n          if(sArray.length>1){\r\n            for(var i in sArray){\r\n              if(i>0){\r\n                sArray[i] = sArray[i].substring(0,1).toUpperCase() + sArray[i].substring(1,sArray[i].length);\r\n              }\r\n            }\r\n            sArray[0] = sArray[0].substring(0,1).toLowerCase()+sArray[0].substring(1,sArray[0].length);\r\n            name = sArray.join(\"\");\r\n            name = name.replace(/\\s+/g, '');\r\n          }else{\r\n            name = name.substring(0,1).toLowerCase()+name.substring(1,name.length);\r\n            name = name.replace(/\\s+/g, '');\r\n          }\r\n\r\n          return name;\r\n\r\n        },\r\n  },\r\n  computed:{\r\n\r\n  },\r\n  watch:{\r\n  }\r\n};\r\n</script>\r\n\r\n<template>\r\n    <div class=\"form-sample\">\r\n        <modal :show=\"showJsonModal\" :name=\"'jsonModel'\" :title=\"modalTitle\" :content=\"modalContent\" @closeModal=\"closeJsonModal\">\r\n        </modal>\r\n        <div v-if=\"!formNotFound\">\r\n        <h2 style=\"text-align:left;\">{{formSample.label.default}}</h2>\r\n        <div class=\"widget-list\">\r\n            <form id=\"form\" v-on:submit.prevent=\"submitFormJson\">\r\n            <div class=\"form-component\" v-for=\"(field,index) in inUseFormSample\" :key=\"index\">\r\n                <div class=\"wrapper-table\" v-if=\"field.type=='text'\">\r\n\t\t\t        <div class=\"item-title\">\r\n\t\t\t\t        {{field.label.default}}\r\n\t\t\t        </div>\r\n\t\t\t        <div class=\"item-content\">\r\n\t\t\t\t        <div class=\"wrapper-input\">\r\n\t\t\t\t\t        <!--<input ng-if=\"field.required==true\" type=\"text\" ng-click=\"mapClientName()\" class=\"form-control intput-text-1\" ng-model=\"field.data\" input-text ng-blur=\"field.data !== undefined && field.value != '' && checkClientName(copyFullName)\" placeholder=\"@Messages(\"createNewClient.requiredMessage\")\" required maxlength=\"{{field.maxLength}} || 200\" autocomplete=\"off\" ng-disabled=\"checkReadOnlyModel()\" name=\"{{field.name}}\" ng-keyup=\"duplicateClientName