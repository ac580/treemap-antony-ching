AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;;AAEA;AACA","file":"TestForm.vue","sourceRoot":"src/views","sourcesContent":["<script>\r\nimport DatePickerCustom from \"@/components/DatePicker.vue\";\r\nimport Modal from \"@/components/Modal.vue\";\r\nimport ApiService from \"@/mixins/apiService\";\r\nimport UploadImage from \"@/components/UploadImage.vue\";\r\nexport default {\r\n  name: \"formSample\",\r\n  components: {\r\n    DatePickerCustom,\r\n    Modal,\r\n    UploadImage\r\n  },\r\n  mixins:[ApiService],\r\n  data: function(){\r\n      return{\r\n          formSample: this.$route.query.formSample,\r\n          currentRadioOption: {},\r\n          currentCheckboxOption: {},\r\n          inUseFormSample:[],\r\n          submitFormData: {},\r\n          showJsonModal: false,\r\n          modalTitle: \"\",\r\n          modalContent: \"\",\r\n          formName: \"\",\r\n          formLabel: \"\",\r\n          formNewName: \"\",\r\n          formNewLabel: \"\",\r\n          action: this.$route.query.action,\r\n          replaceNewFormName: false,\r\n      } \r\n  },\r\n  created: function(){\r\n      \r\n    console.log(\"running test form\",this.formSample);\r\n    this.formSample = JSON.parse(this.formSample);\r\n\r\n    if(this.$store.state['formNameChanged']==\"\"){\r\n        this.formName = this.formSample['name'];\r\n        this.formLabel = this.formSample['label'];\r\n        this.replaceNewFormName = false;\r\n    }else{\r\n        var parseObj = JSON.parse(this.$store.state['formNameChanged']);\r\n        this.formName = parseObj['pName'];\r\n        this.formLabel = parseObj['pLabel'];\r\n\r\n        this.formNewName = parseObj['name'];\r\n        this.formNewLabel = parseObj['label'];\r\n        this.replaceNewFormName = true;\r\n       \r\n    }\r\n\r\n    this.inUseFormSample = this.formSample['elements'];\r\n    this.$store.commit(\"formInUseDefault\", JSON.stringify(this.inUseFormSample));\r\n    \r\n  },\r\n  methods: {\r\n        getPlaceholder(bool){ //in component maybe\r\n            if(bool==true){\r\n                return \"(Required)\";\r\n            }else{\r\n                return \"\";\r\n            }\r\n        },\r\n        checkRadioClick(indexRadio,id){\r\n            this.currentRadioOption = {};\r\n            this.currentRadioOption = this.inUseFormSample.filter((node) => node.id === id)[0];\r\n            var filterOption =  this.currentRadioOption.options;\r\n\r\n            for(var i in filterOption){\r\n                filterOption[i]['checked'] = false;\r\n            }\r\n            filterOption[indexRadio]['checked'] = true;\r\n            this.currentRadioOption.options = filterOption;\r\n            this.inUseFormSample.find(v => v.id == id).options = this.currentRadioOption.options;\r\n\r\n        },\r\n        checkMultipleCheckboxClick(indexCheckbox,id){\r\n            this.currentCheckboxOption = {};\r\n            this.currentCheckboxOption = this.inUseFormSample.filter((node) => node.id === id)[0];\r\n            var filterOption =  this.currentCheckboxOption.options;\r\n\r\n            filterOption[indexCheckbox]['checked'] = !filterOption[indexCheckbox]['checked'];\r\n    \r\n            this.currentCheckboxOption.options = filterOption;\r\n            this.inUseFormSample.find(v => v.id == id).options = this.currentCheckboxOption.options;\r\n        },\r\n        submitFormJson(bool){\r\n            for(var i in this.inUseFormSample){\r\n                if(this.inUseFormSample[i].type==\"radio\"){\r\n                    for(var h in this.inUseFormSample[i]['options']){\r\n                        if(this.inUseFormSample[i]['options'][h]['checked']==true){\r\n                            this.submitFormData[this.inUseFormSample[i].label.default] = this.inUseFormSample[i]['options'][h]['value']\r\n                        }\r\n                    }   \r\n                }else if(this.inUseFormSample[i].type==\"mobile\"){\r\n                    this.submitFormData[this.inUseFormSample[i].label.default] = {\r\n                        \"countryCode\": this.inUseFormSample[i]['prefix'][0]['data'],\r\n                        \"number\": this.inUseFormSample[i]['prefix'][1]['data']\r\n                    };\r\n                }else if(this.inUseFormSample[i].type==\"select\"){\r\n                    this.submitFormData[this.inUseFormSample[i].label.default] = this.inUseFormSample[i].data || \"\";\r\n           \r\n                }else if(this.inUseFormSample[i].type==\"checkbox\"){\r\n                    var dataArray = [];\r\n                    if(this.inUseFormSample[i]['options']){\r\n                        for(var j in this.inUseFormSample[i]['options']){\r\n                            if(this.inUseFormSample[i]['options'][j]['checked']==true){\r\n                                dataArray.push(this.inUseFormSample[i]['options'][j]['value']);\r\n                            }\r\n                        }\r\n                        if(dataArray.length>0){\r\n                            this.submitFormData[this.inUseFormSample[i].label.default] = dataArray;\r\n                        }else{\r\n                            this.submitFormData[this.inUseFormSample[i].label.default] = \"\";\r\n                        }\r\n                    }else{\r\n                        this.submitFormData[this.inUseFormSample[i].label.default] = this.inUseFormSample[i].data || \"\";\r\n                    }\r\n                }else if(this.inUseFormSample[i].type==\"paneldynamic\"){\r\n                    var valueObject = {};\r\n                    for(var k in this.inUseFormSample[i]['fields']){\r\n                        valueObject[this.inUseFormSample[i]['fields'][k]['label']['default']] = this.inUseFormSample[i]['fields'][k]['data'];\r\n                    }\r\n\r\n                    this.submitFormData[this.inUseFormSample[i].label.default] = valueObject;\r\n                }else if(this.inUseFormSample[i].type==\"html\"){\r\n                    this.submitFormData[this.inUseFormSample[i].label.default] = this.inUseFormSample[i].html.substring(0,10)+\"...\" || \"\";\r\n                }else{\r\n                    this.submitFormData[this.inUseFormSample[i].label.default] = this.inUseFormSample[i].data || \"\";\r\n                }\r\n            }\r\n            this.modalTitle = \"Form Json\";\r\n            this.modalContent = JSON.stringify(this.submitFormData);\r\n            this.showJsonModal = true;\r\n            return bool;\r\n        },\r\n        closeJsonModal(bool){\r\n            this.showJsonModal = bool;\r\n            return bool;\r\n        },\r\n        clearField(){\r\n            this.inUseFormSample = {};\r\n            this.inUseFormSample = JSON.parse(this.$store.state['formInUseDefault']);\r\n        },\r\n        backToEdit(){\r\n            this.$router.go(-1)\r\n        },\r\n        saveForm(action){\r\n            let saveFormJson = null;\r\n\r\n            if(this.replaceNewFormName){\r\n                saveFormJson = {\"formName\": this.formNewName, \"label\": {\"default\": this.formNewLabel, \"zhTw\":\"\"},\"elements\": JSON.parse(this.$store.state.formInUseDefault)};\r\n            }else{\r\n                saveFormJson = {\"formName\": this.formName, \"label\": {\"default\": this.formLabel, \"zhTw\":\"\"},\"elements\": JSON.parse(this.$store.state.formInUseDefault)};\r\n            }\r\n\r\n            let formBuildJsonParse = JSON.parse(this.$store.state.formBuildJson);\r\n            var existedFormsArray = formBuildJsonParse['pages'];\r\n\r\n            switch(action){\r\n                case \"create\":\r\n                    existedFormsArray[existedFormsArray.length-1] = saveFormJson;\r\n            \r\n                    this.$store.commit(\"formBuildJson\", JSON.stringify(formBuildJsonParse));\r\n\r\n                    this.saveJsonToServer(formBuildJsonParse);\r\n                    break;\r\n                case \"edit\":\r\n                    if(existedFormsArray.length>0){\r\n\r\n                        var checkFormIsExisted = [];\r\n\r\n                        for(var i in existedFormsArray){\r\n                            if(existedFormsArray[i]['formName'] == this.formName){\r\n                                checkFormIsExisted.push(true);\r\n                            }else{\r\n                                checkFormIsExisted.push(false);\r\n                            }\r\n                        }\r\n\r\n                        if(checkFormIsExisted.indexOf(true)==-1){\r\n                            existedFormsArray.push(saveFormJson);\r\n                        }else{\r\n                            var replaceIndex =  checkFormIsExisted.indexOf(true);\r\n                            existedFormsArray[replaceIndex] = saveFormJson;\r\n                        }\r\n\r\n                    }else{\r\n                        existedFormsArray.push(saveFormJson);\r\n                    }\r\n                    this.saveJsonToServer(formBuildJsonParse);\r\n                break;\r\n            }   \r\n        },\r\n        saveJsonToServer(json){\r\n            var saveApi = this.saveFormBuilder(\"tester\",JSON.stringify(json));\r\n            saveApi.then(res=>{\r\n                if(res.status==200){\r\n                    this.$store.commit(\"formBuildJson\", JSON.stringify(json));\r\n                    if(this.replaceNewFormName){\r\n                        alert(\"Successfully saved form - \"+this.formNewLabel+\"!\");\r\n                    }else{\r\n                        alert(\"Successfully saved form - \"+this.formLabel+\"!\");\r\n                    }\r\n                }else{\r\n                    alert(\"Failed to save json to server.\");\r\n                }\r\n            });\r\n        },\r\n        checkCheckboxChecked(list,bool){\r\n            var boolList = [];\r\n            if(bool==true){\r\n                for(var i in list){\r\n                    boolList.push(list[i][\"checked\"]);\r\n                }\r\n\r\n                if(boolList.indexOf(true)==-1){\r\n                    return true;\r\n                }else{\r\n                    return false;\r\n                }\r\n            }else{\r\n                return false;\r\n            }  \r\n        },\r\n        onChange(image) {\r\n            console.log('New picture selected!')\r\n            if (image.file) {\r\n                console.log('Picture loaded.',image.index)\r\n                this.inUseFormSample[image.index][\"data\"] = image.name;\r\n            }else{\r\n                this.inUseFormSample[image.index][\"data\"] = \"\";\r\n            }\r\n        },\r\n\r\n  },\r\n  computed:{\r\n\r\n  },\r\n  watch:{\r\n      \r\n  }\r\n};\r\n</script>\r\n\r\n<template>\r\n    <div class=\"test-form\">\r\n        <modal :show=\"showJsonModal\" :name=\"'jsonModel'\" :title=\"modalTitle\" :content=\"modalContent\" @closeModal=\"closeJsonModal\">\r\n        </modal>\r\n        <div class=\"header\">\r\n            <h2 style=\"text-align:left;\">{{formSample.label}}</h2>\r\n            <button v-on:click=\"backToEdit\">Edit Form</button>\r\n            <button v-on:click=\"saveForm(action)\">Save Form</button>\r\n        </div>\r\n        <div class=\"widget-list\">\r\n            <form id=\"form\" v-on:submit.prevent=\"submitFormJson\">\r\n            <div class=\"form-component\" v-for=\"(field,index) in inUseFormSample\" :key=\"index\">\r\n                <div class=\"wrapper-table\" v-if=\"field.type=='text'\">\r\n\t\t\t        <div class=\"item-title\">\r\n\t\t\t\t        {{field.label.default}}\r\n\t\t\t        </div>\r\n\t\t\t        <div class=\"item-content\">\r\n\t\t\t\t        <div class=\"wrapper-input\">\r\n\t\t\t\t\t        <!--<input ng-if=\"field.required==true\" type=\"text\" ng-click=\"mapClientName()\" class=\"form-control intput-text-1\" ng-model=\"field.data\" input-text ng-blur=\"field.data !== undefined && field.value != '' && checkClientName(copyFullName)\" placeholder=\"@Messages(\"createNewClient.requiredMessage\")\" required maxlength=\"{{field.maxLength}} || 200\" autocomplete=\"off\" ng-disabled=\"checkReadOnlyModel()\" name=\"{{field.name}}\" ng-keyup=\"duplicateClientName=false\" limit-maxlength/>\r\n\t\t\t\t\t        <input ng-if=\"field.required==false\" type=\"text\" ng-click=\"mapClientName()\" class=\"form-control intput-text-1\" ng-model=\"field.data\" input-text ng-blur=\"field.data !== undefined && field.value != '' && checkClientName(copyFullName)\" maxlength=\"{{field.maxLength}} || 200\" autocomplete=\"off\" ng-disabled=\"checkReadOnlyModel()\" name=\"{{field.name}}\" ng-keyup=\"duplicateClientName=false\" limit-maxlength/>-->\r\n                            <input v-bind:id=\"field.name\" type=\"text\" class=\"form-control\" v-model=\"field.data\" input-text  v-bind:required=\"field.required\" v-bind:placeholder=\"getPlaceholder(field.required)\"  v-bind:maxlength=\"field.maxLength || 500\" v-bind:minlength=\"field.minLength || null\" autocomplete=\"off\" v-bind:name=\"field.name\" limit-maxlength/>\r\n                        </div>\r\n\t\t\t\t    </div>\r\n\t\t\t    </div>\r\n                <div class=\"wrapper-table\" v-else-if=\"field.type=='number'\">\r\n\t\t\t        <div class=\"item-title\">\r\n\t\t\t\t        {{field.label.default}}\r\n\t\t\t        </div>\r\n\t\t\t        <div class=\"item-content\">\r\n\t\t\t\t        <div class=\"wrapper-input\">\r\n                            <input v-bind:id=\"field.name\" type=\"number\" class=\"form-control\" v-bind:min=\"field.minValue || null\" v-bind:max=\"field.maxValue || null\" v-model=\"field.data\" input-text  v-bind:required=\"field.required\" v-bind:placeholder=\"getPlaceholder(field.required)\" autocomplete=\"off\" v-bind:name=\"field.name\"/>\r\n                        </div>\r\n\t\t\t\t    </div>\r\n\t\t\t    </div>\r\n                <div class=\"wrapper-table\" v-else-if=\"field.type=='imageFile'\">\r\n\t\t\t        <div class=\"item-title\">\r\n\t\t\t\t        {{field.label.default}}\r\n\t\t\t        </div>\r\n\t\t\t        <div class=\"item-content\">\r\n\t\t\t\t        <div class=\"wrapper-input\">\r\n                            <upload-image :rowIndex=\"index\" @change=\"onChange\" \r\n                            accept=\"image/jpg, image/png\" ></upload-image>\r\n                        </div>\r\n\t\t\t\t    </div>\r\n\t\t\t    </div>\r\n                <div class=\"wrapper-table\" v-else-if=\"field.type=='radio'\">\r\n\t\t\t        <div class=\"item-title\">\r\n\t\t\t\t        {{field.label.default}}\r\n\t\t\t        </div>\r\n\t\t\t        <div class=\"item-content\">\r\n\t\t\t\t        <label v-for=\"(option,indexRadio) in field.options\" :key=\"indexRadio\">\r\n\t\t\t\t            <input v-bind:id=\"option.id\" class=\"radio-custom\" type=\"radio\" v-bind:required=\"field.required\" v-bind:name=\"field.name\" v-bind:value=\"option.data\" v-bind:checked=\"option.checked\" v-on:click=\"checkRadioClick(indexRadio,field.id)\">\r\n\t\t\t\t\t        <label v-bind:for=\"option.id\">{{ option.label.default }}</label>\r\n\t\t\t\t        </label>\r\n\t\t\t        </div>\r\n\t\t        </div>\r\n                <div class=\"wrapper-table\" v-else-if=\"field.type=='checkbox'\">\r\n\t\t\t        <div class=\"item-title\">\r\n\t\t\t\t        {{field.label.default}}\r\n\t\t\t        </div>\r\n\t\t\t        <div class=\"item-content\" v-if=\"field.options\">\r\n\t\t\t\t        <label v-for=\"(option,indexCheckbox) in field.options\" :key=\"indexCheckbox\">\r\n\t\t\t\t            <input v-bind:id=\"option.id\" class=\"checkbox-custom\" type=\"checkbox\" v-bind:name=\"field.name\" v-bind:value=\"option.value\" v-bind:checked=\"option.checked\" v-on:click=\"checkMultipleCheckboxClick(indexCheckbox,field.id)\">\r\n\t\t\t\t\t        <label v-bind:for=\"option.id\" class=\"checkbox-custom-label\">{{ option.label.default }}</label>\r\n\t\t\t\t        </label>\r\n\r\n                        <p v-if=\"checkCheckboxChecked(field.options, field.required)\">You must select at least 1 checkbox.</p>\r\n                    </div>\r\n                    <div class=\"item-content\" v-else>\r\n\t\t\t\t        <input v-bind:id=\"field.id\" class=\"checkbox-custom\" v-bind:name=\"field.name\" type=\"checkbox\" v-model=\"field.data\" v-bind:value=\"field.data\"/>\r\n                        <label v-bind:for=\"field.id\" class=\"checkbox-custom-label\">&nbsp;</label>\r\n\t\t\t        </div>\r\n                </div>\r\n                <div class=\"wrapper-table\" v-else-if=\"field.type=='mobile'\">\r\n\t\t\t        <div class=\"item-title\">\r\n\t\t\t\t        {{field.label.default}}\r\n\t\t\t        </div>\r\n\t\t\t        <div class=\"item-content-mobile\">\r\n\t\t\t\t        <div class=\"wrapper-input-cc\">\r\n\t\t\t\t\t        <input type=\"tel\" class=\"form-control\" v-model=\"field.prefix[0].data\" input-text  v-bind:maxlength=\"field.prefix[0].maxLength\" string-to-number autocomplete=\"off\"/>  \r\n\t\t\t\t        </div>\r\n\t\t\t\t        <div class=\"wrapper-input-number\">\r\n\t\t\t\t\t        <input type=\"tel\" class=\"form-control\" v-model=\"field.prefix[1].data\" input-text  v-bind:maxlength=\"field.prefix[1].maxLength\" string-to-number autocomplete=\"off\"/>\r\n\t\t\t\t\t\r\n\t\t\t\t        </div>\r\n\t\t\t        </div>\r\n\t\t        </div>\r\n                <div class=\"wrapper-table\" v-else-if=\"field.type=='select'\">\r\n                    <div class=\"item-title\">\r\n\t\t\t\t        {{field.label.default}}\r\n\t\t\t        </div>\r\n                    <div class=\"item-content\">\r\n                        <select v-bind:name=\"field.name\" v-model=\"field.data\" v-bind:required=\"field.required\" class=\"form-control\">\r\n                            <option v-bind:disabled=\"field.required\" value=\"\">Please select one</option>\r\n                            <option v-for=\"(option,indexSelect) in field.options\" :key=\"indexSe