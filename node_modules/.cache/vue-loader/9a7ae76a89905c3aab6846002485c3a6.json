ACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA","file":"CreateForm.vue","sourceRoot":"src/views","sourcesContent":["<script>\r\n//import DatePickerCustom from \"@/components/DatePicker.vue\";\r\nimport Modal from \"@/components/Modal.vue\";\r\nimport ListDraggable from \"@/components/ListDraggable.vue\";\r\nexport default {\r\n  name: \"createForm\",\r\n  components: {\r\n    //DatePickerCustom,\r\n    Modal,\r\n    ListDraggable\r\n  },\r\n  data: function(){\r\n        return{\r\n            name: this.$route.query.name,\r\n            label: this.$route.query.label,\r\n            action: this.$route.query.action,\r\n            showJsonModal: false,\r\n            modalTitle: \"\",\r\n            modalContent: \"\",\r\n            formIsEmpty: true,\r\n            duplicateFieldName: false,\r\n            optionList:[ \r\n                { \"name\": \"html\", \"id\": \"html\", \"type\": \"html\", \"label\":{\"default\":\"HTML\",\"zhTw\":\"\"}, \"html\": \"\" },\r\n                { \"name\": \"textField\", \"id\": \"textField\", \"type\": \"text\", \"label\":{\"default\":\"Text Field\",\"zhTw\":\"\"} },\r\n                { \"name\": \"numberField\", \"id\": \"numberField\", \"type\": \"number\", \"label\":{\"default\":\"Number Field\",\"zhTw\":\"\"}},\r\n                { \"name\": \"singleCheckbox\", \"id\": \"singleCheckbox\", \"type\": \"singleCheckbox\", \"label\":{\"default\":\"Single Checkbox\",\"zhTw\":\"\"}},\r\n                { \"name\": \"multipleCheckboxes\", \"id\": \"multipleCheckboxes\", \"type\": \"multipleCheckboxes\", \"label\":{\"default\":\"Multiple Checkboxes\",\"zhTw\":\"\"}, \"options\":[{\"id\":\"item1\", \"name\":\"item 1\", \"value\":\"item 1\", \"label\":{\"default\":\"item 1\",\"zhTw\":\"\"},\"checked\":false},\r\n                {\"id\":\"item2\",\"name\":\"item 2\",\"value\":\"item 2\", \"label\":{\"default\":\"item 2\",\"zhTw\":\"\"},\"checked\":false},{\"id\":\"item3\",\"name\":\"item 3\",\"value\":\"item 3\", \"label\":{\"default\":\"item 3\",\"zhTw\":\"\"},\"checked\":false}]},\r\n                { \"name\": \"radio\", \"id\": \"radio\", \"type\": \"radio\", \"label\":{\"default\":\"Radio\",\"zhTw\":\"\"}, \"options\":[{\"id\":\"item1\", \"name\":\"item 1\",\"value\":\"item 1\", \"label\":{\"default\":\"item 1\",\"zhTw\":\"\"},\"checked\":false},{\"id\":\"item2\",\"name\":\"item 2\",\"value\":\"item 2\", \"label\":{\"default\":\"item 2\",\"zhTw\":\"\"},\"checked\":false},\r\n                {\"id\":\"item3\",\"name\":\"item 3\",\"value\":\"item 3\", \"label\":{\"default\":\"item 3\",\"zhTw\":\"\"},\"checked\":false}]},\r\n                { \"name\": \"dropdown\", \"id\": \"select\", \"type\": \"select\", \"label\":{\"default\":\"Dropdown\",\"zhTw\":\"\"}, \"options\":[{\"id\":\"item1\", \"name\":\"item 1\",\"value\":\"item 1\", \"label\":{\"default\":\"item 1\",\"zhTw\":\"\"},\"checked\":false},{\"id\":\"item2\",\"name\":\"item 2\",\"value\":\"item 2\", \"label\":{\"default\":\"item 2\",\"zhTw\":\"\"},\"checked\":false},\r\n                {\"id\":\"item3\",\"name\":\"item 3\",\"value\":\"item 3\", \"label\":{\"default\":\"item 3\",\"zhTw\":\"\"},\"checked\":false}]},\r\n                { \"name\": \"textarea\", \"id\": \"textarea\", \"label\":{\"default\":\"Textarea\",\"zhTw\":\"\"}, \"type\": \"textarea\" },\r\n                { \"name\": \"mobile\", \"id\": \"mobile\", \"label\":{\"default\":\"Mobile\",\"zhTw\":\"\"}, \"type\": \"mobile\", \"prefix\": [\r\n                    {\"id\":\"counytryCode\",\"name\":\"counytryCode\", \"data\":\"\", \"maxLength\":4},\r\n                    {\"id\":\"number\",\"name\":\"number\", \"data\":\"\", \"maxLength\":20}\r\n                ]},\r\n                { \"name\": \"date\", \"id\": \"date\", \"type\": \"date\", \"config\":\"\", \"label\":{\"default\":\"Date\",\"zhTw\":\"\"},},\r\n                { \"name\": \"email\", \"id\": \"email\", \"type\": \"email\", \"label\":{\"default\":\"Email\",\"zhTw\":\"\"}, },\r\n                { \"name\": \"imageFile\", \"id\": \"imageFile\", \"type\": \"imageFile\", \"label\":{\"default\":\"Image File\",\"zhTw\":\"\"} },\r\n                { \"name\": \"paneldynamic\", \"id\": \"paneldynamic\", \"label\":{\"default\":\"Dynamic Panel\",\"zhTw\":\"\"}, \"type\": \"paneldynamic\", \"fields\":[{ \"name\": \"Item 1\", \"id\": \"item1\", \"type\": \"text\", \"required\": false, \"label\":{\"default\":\"Item 1\",\"zhTw\":\"\"} }] }],\r\n            fieldListFormat: [],\r\n            fieldList:[],\r\n            pageTitle: \"\",\r\n            showEditFormNameModal: false,\r\n            formNameArray: [],\r\n            editFormNameObj: null,\r\n            htmlIsEmpty:false\r\n        }  \r\n  },\r\n  created: function(){\r\n    console.log(\"running createForm\",this.name);\r\n    this.editFormNameObj = JSON.parse(this.$store.state.formBuildJson);\r\n\r\n    if(this.$store.state['formNameChanged']!==\"\"){\r\n        var parseObj = JSON.parse(this.$store.state['formNameChanged']);\r\n        if(parseObj['status']==true){\r\n            this.name = parseObj['name'];\r\n            this.label = parseObj['label'];\r\n        }\r\n    }\r\n\r\n    if(this.$store.state['formForEdit']!==\"\"){\r\n        var formatJson = JSON.parse(this.$store.state['formForEdit']);\r\n        this.fieldList = formatJson['elements'];\r\n        this.returnForm(this.fieldList);\r\n        this.formatFieldType('edit');\r\n    }\r\n\r\n    if(this.action=='create'){\r\n        this.pageTitle = \"Create form - \" + this.label;\r\n    }else{\r\n        this.pageTitle = \"Edit form - \" + this.label;\r\n    }\r\n  },\r\n  methods: {\r\n        closeJsonModal(bool){\r\n            this.showJsonModal = bool;\r\n            return bool;\r\n        },\r\n        returnForm(list){\r\n            console.log(\"return list\",list)\r\n            if(list.length==0){\r\n                this.formIsEmpty = true;\r\n                this.duplicateFieldName = false;\r\n            }else{\r\n                let findDuplicates = arr => arr.filter((item, index) => arr.indexOf(item) != index);\r\n                var fieldNameArrayPassed = [];\r\n                for(var i in list){\r\n                    fieldNameArrayPassed.push(list[i][\"name\"]);\r\n                }\r\n\r\n                if(findDuplicates(fieldNameArrayPassed).length>0){\r\n                    this.formIsEmpty = true;\r\n                    this.duplicateFieldName = true;\r\n                }else{\r\n                    this.formIsEmpty = false;\r\n                    this.duplicateFieldName = false;\r\n                }\r\n            }\r\n\r\n        },\r\n        createNewForm(){\r\n            this.fieldListFormat = [];\r\n            for(var i in this.fieldList){\r\n                var isRequired = false;\r\n                if(this.fieldList[i][\"required\"] && this.fieldList[i][\"required\"]==true){\r\n                    isRequired = true;\r\n                }\r\n\r\n                this.fieldListFormat.push(\r\n                    {\r\n                        \"id\":this.fieldList[i][\"id\"],\r\n                        \"type\": this.fieldList[i][\"type\"],\r\n                        \"name\": this.fieldList[i][\"name\"], \r\n                        \"required\": isRequired,\r\n                        \"label\": this.fieldList[i][\"label\"],\r\n                        \"data\":\"\"\r\n                    }\r\n                );\r\n            }\r\n\r\n            this.formatFieldType('create');\r\n            this.addOptions();\r\n            this.addHtml();\r\n            this.addOtherProps();\r\n\r\n            var jsonList = {\"name\":this.name, \"label\":this.label, \"elements\":this.fieldListFormat}\r\n            this.$store.commit(\"formForEdit\", JSON.stringify(jsonList));\r\n            if(!this.htmlIsEmpty){\r\n                this.$router.push({ path: \"testForm\", query: { formSample: JSON.stringify(jsonList), action: this.action } });\r\n            }\r\n            \r\n        },\r\n        formatFieldType(type){\r\n            if(type=='create'){\r\n                for(var i in this.fieldListFormat){\r\n                    if(this.fieldListFormat[i][\"type\"]==\"singleCheckbox\"){\r\n                        this.fieldListFormat[i][\"type\"] = \"checkbox\";\r\n                    }else if(this.fieldListFormat[i][\"type\"]==\"multipleCheckboxes\"){\r\n                        this.fieldListFormat[i][\"type\"] = \"checkbox\";\r\n                    }\r\n                }\r\n            }else{\r\n                for(var j in this.fieldList){\r\n                    if(this.fieldList[j][\"type\"]==\"checkbox\"){\r\n                      if(this.fieldList[j][\"options\"]){\r\n                        this.fieldList[j][\"type\"] = \"multipleCheckboxes\";\r\n                      }else{\r\n                        this.fieldList[j][\"type\"] = \"singleCheckbox\";\r\n                      }\r\n\r\n                    }\r\n                }\r\n            }\r\n        },\r\n        addOptions(){\r\n            for(var i in this.fieldListFormat){\r\n                if(this.fieldListFormat[i][\"type\"]==\"select\" || this.fieldListFormat[i][\"type\"]==\"checkbox\" || this.fieldListFormat[i][\"type\"]==\"radio\" || this.fieldListFormat[i][\"type\"]==\"mobile\"){\r\n                    \r\n                    if(this.fieldList[i][\"options\"]){\r\n                        this.fieldListFormat[i][\"options\"] = this.fieldList[i][\"options\"];\r\n                    }else if(this.fieldList[i][\"prefix\"]){\r\n                        this.fieldListFormat[i][\"prefix\"] = this.fieldList[i][\"prefix\"];\r\n                    }\r\n                    \r\n                }else if(this.fieldListFormat[i][\"type\"]==\"paneldynamic\"){\r\n                    if(this.fieldList[i][\"fields\"]){\r\n                        this.fieldListFormat[i][\"fields\"] = this.fieldList[i][\"fields\"];\r\n                    }\r\n                }\r\n            }\r\n        },\r\n        addHtml(){\r\n            var checkHtmlArray = [];\r\n            for(var i in this.fieldListFormat){\r\n                if(this.fieldListFormat[i][\"type\"]==\"html\"){\r\n                    if(this.fieldList[i][\"html\"]){\r\n                        this.fieldListFormat[i][\"html\"] = this.fieldList[i][\"html\"];\r\n                        console.log(\"show\",this.fieldListFormat[i][\"html\"])\r\n                        checkHtmlArray.push(true);\r\n                    }else{\r\n                        checkHtmlArray.push(false);\r\n                    }   \r\n                }\r\n            }\r\n\r\n            console.log(\"ok check\",checkHtmlArray)\r\n            if(checkHtmlArray.length==0){\r\n                this.htmlIsEmpty = false;\r\n            }else{\r\n                if(checkHtmlArray.indexOf(false)!=-1){\r\n                    this.htmlIsEmpty = true;\r\n                }else{\r\n                    this.htmlIsEmpty = false;\r\n                }\r\n\r\n            }\r\n\r\n        },\r\n        addOtherProps(){\r\n            for(var i in this.fieldListFormat){\r\n                if(this.fieldList[i][\"maxLength\"]){\r\n                        this.fieldListFormat[i][\"maxLength\"] = this.fieldList[i][\"maxLength\"];\r\n                }\r\n                if(this.fieldList[i][\"minLength\"]){\r\n                    this.fieldListFormat[i][\"minLength\"] = this.fieldList[i][\"minLength\"];\r\n                }\r\n                if(this.fieldList[i][\"maxValue\"]){\r\n                        this.fieldListFormat[i][\"maxValue\"] = this.fieldList[i][\"maxValue\"];\r\n                }\r\n                if(this.fieldList[i][\"minValue\"]){\r\n                    this.fieldListFormat[i][\"minValue\"] = this.fieldList[i][\"minValue\"];\r\n                }\r\n                if(this.fieldList[i][\"config\"]){\r\n                    this.fieldListFormat[i][\"config\"] = this.fieldList[i][\"config\"];\r\n                }\r\n                \r\n            }\r\n        },\r\n        clearForm(){\r\n            this.$store.commit(\"formForEdit\", \"\");\r\n            this.fieldList = [];\r\n        },\r\n        openFormModel(){\r\n            this.modalTitle = \"Change form name - \"+this.label;\r\n            this.modalContent = \"\";\r\n            \r\n            this.formNameArray = [];\r\n\r\n            for(var i in this.editFormNameObj['pages']){\r\n                this.formNameArray.push(this.editFormNameObj['pages'][i]['formName'].toLowerCase());\r\n            }\r\n\r\n            this.showEditFormNameModal = true;\r\n            return true;\r\n        },\r\n        closeEditFormModalCancel(){\r\n            this.showEditFormNameModal = false;\r\n            return false;\r\n        },\r\n        editFormName(name,label){\r\n            var pName = this.name;\r\n            var pLabel = this.label;\r\n            this.name = name;\r\n            this.label = label;\r\n\r\n            if(this.action==\"create\"){\r\n                this.pageTitle = \"Create form - \" + this.label;\r\n            }else{\r\n                this.pageTitle = \"Edit form - \" + this.label;\r\n            }\r\n            this.$store.commit(\"formNameChanged\",JSON.stringify({\"status\": true,\"name\":name, \"label\":label, \"pName\": pName, \"pLabel\": pLabel,\"action\":this.action}));\r\n            \r\n            this.showEditFormNameModal = false;\r\n            return false;\r\n        }\r\n\r\n  },\r\n  computed:{\r\n\r\n  },\r\n  watch:{\r\n  }\r\n};\r\n</script>\r\n\r\n<template>\r\n    <div class=\"create-form\">\r\n        <modal :show=\"showEditFormNameModal\" :name=\"'editFormNameModel'\" :buttonName=\"'OK'\" :fieldNameArray=\"formNameArray\" :title=\"modalTitle\" :content=\"modalContent\" @closeModal=\"editFormName\" @closeModalCancel=\"closeEditFormModalCancel\">\r\n        </modal>\r\n        <div style=\"display:flex; justify-content:center;\">\r\n            <h2>{{pageTitle}} </h2>\r\n            <p style=\"text-decoration:underline; color:#00AEEF; font-weight:400; margin-left: 5px; cursor: pointer;\" @click=\"openFormModel\">Edit name</p>\r\n        </div>\r\n        <button :disabled=\"formIsEmpty\" @click=\"createNewForm\">Test Your Form!</button>\r\n        <p v-show=\"htmlIsEmpty\" class=\"input-warning\">Please input something in the html editor!</p>\r\n        <button :disabled=\"formIsEmpty\" @click=\"clearForm\">Clear</button>\r\n        <p v-show=\"duplicateFieldName\" class=\"input-warning\">Can't have duplicated field's name in same form!</p>\r\n        <p v-show=\"duplicateFieldName\" class=\"input-warning-sub\">Click field name to edit the name for each field</p>\r\n        <div class=\"container\">\r\n            <ListDraggable :list1=\"optionList\" :list2=\"fieldList\" @returnFieldList=\"returnForm\"></ListDraggable>\r\n        </div>\r\n        \r\n    </div>\r\n</template>"]}]}