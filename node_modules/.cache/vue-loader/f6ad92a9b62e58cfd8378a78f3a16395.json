;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"ListDraggable.vue","sourceRoot":"src/components","sourcesContent":["<script>\r\nimport draggable from \"vuedraggable\";\r\nimport Modal from \"@/components/Modal.vue\";\r\n//import { VueEditor } from \"vue2-editor\";\r\nexport default {\r\n  name: \"listDraggable\",\r\n  display: \"ListDraggable\",\r\n  order: 2,\r\n  components: {\r\n    draggable,\r\n    Modal,\r\n    //VueEditor,\r\n\r\n  },\r\n   props: {\r\n    list1: {\r\n      type: Array,\r\n      required: true,\r\n    },\r\n    list2:{\r\n      type: Array,\r\n      required: true,\r\n    },\r\n   },\r\n  data() {\r\n    return {\r\n      showChangeNameModal: false,\r\n      showEditModal: false,\r\n      modalTitle: \"\",\r\n      modalContent: \"\",\r\n      changeNameIndex: null,\r\n      showCancelIndex: null,\r\n      fieldNameArray: null,\r\n      currentFieldObj: {},\r\n      oldListLength: null\r\n    };\r\n  },\r\n  created: function(){\r\n  },\r\n  methods: {\r\n    log: function(evt) {\r\n      window.console.log(evt);\r\n    },\r\n    addItemToList: function(index){\r\n      if(this.list1[index]['type']==\"select\" || this.list1[index]['type']==\"multipleCheckboxes\" || this.list1[index]['type']==\"radio\"){\r\n        this.list2.push({\"name\": this.list1[index]['name'], \"id\": this.list1[index]['id'], \"type\": this.list1[index]['type'], \"options\": this.list1[index]['options'], \"label\": this.list1[index]['label']});\r\n      }else if(this.list1[index]['type']==\"paneldynamic\"){\r\n        this.list2.push({\"name\": this.list1[index]['name'], \"id\": this.list1[index]['id'], \"type\": this.list1[index]['type'], \"required\": false, \"fields\": this.list1[index]['fields'], \"label\": this.list1[index]['label']});\r\n      }else if(this.list1[index]['type']==\"mobile\"){\r\n        this.list2.push({\"name\": this.list1[index]['name'], \"id\": this.list1[index]['id'], \"type\": this.list1[index]['type'], \"prefix\": this.list1[index]['prefix'], \"label\": this.list1[index]['label']});\r\n      }else if(this.list1[index]['type']==\"html\"){\r\n        // add html modal\r\n        this.list2.push({\"name\": this.list1[index]['name'], \"id\": this.list1[index]['id'], \"type\": this.list1[index]['type'], \"label\": this.list1[index]['label']});\r\n      }\r\n      else{\r\n        this.list2.push({\"name\": this.list1[index]['name'], \"id\": this.list1[index]['id'], \"type\": this.list1[index]['type'], \"label\": this.list1[index]['label']});\r\n      }\r\n    },\r\n    removeElementFromFieldList: function(index){\r\n      this.list2.splice(index,1);\r\n    },\r\n    changeFieldName: function(index){\r\n        this.fieldNameArray = [];\r\n        for(var i in this.list2){\r\n          this.fieldNameArray.push(this.list2[i]['label']['default'].toLowerCase());\r\n        }\r\n        this.showChangeNameModal = true;\r\n        this.changeNameIndex = index;\r\n        var fieldNum = parseInt(index)+1;\r\n        this.modalTitle = \"Change Field's name (\"+fieldNum+\")\";\r\n        this.modalContent = \"\";\r\n    },\r\n    closeChangeNameModal(nameAndId){\r\n      this.list2[this.changeNameIndex ][\"name\"] = nameAndId['id'];\r\n      this.list2[this.changeNameIndex ][\"id\"] = nameAndId['id'];\r\n      this.list2[this.changeNameIndex ][\"label\"] = nameAndId['label'];\r\n      this.$emit('returnFieldList', this.list2);\r\n      this.showChangeNameModal = false;;\r\n      return false;\r\n    },\r\n    closeChangeNameModalCancel(bool){\r\n      this.showChangeNameModal = bool;\r\n      return bool;\r\n    },\r\n    addFieldInPanel(index){\r\n      var indexNum = parseInt(this.list2[index][\"fields\"].length)+1;\r\n      var list2EleArray = this.list2[index][\"fields\"];\r\n\r\n      this.list2[index][\"fields\"] = [];\r\n      for(var i in list2EleArray){\r\n        this.list2[index][\"fields\"].push(list2EleArray[i]);\r\n      }\r\n\r\n      this.list2[index][\"fields\"].push({ \"name\": \"Item \"+ indexNum, \"id\": \"item\"+indexNum, \"type\": this.list2[index][\"fields\"][0][\"type\"], \"required\": false, \"label\":{\"default\":\"Item \"+ indexNum,\"zhTw\":\"\"} });\r\n      \r\n      var list1Length = parseInt(this.list1.length)-1;\r\n      var list1EleArray = this.list1[list1Length][\"fields\"][0];\r\n\r\n      this.list1[list1Length][\"fields\"] = [];\r\n      this.list1[list1Length][\"fields\"].push(list1EleArray);\r\n    },\r\n    deleteFieldInPanel(index){\r\n      this.list2[index][\"fields\"].splice(-1, 1);    \r\n    },\r\n    editElementFromFieldList(index,eleType){\r\n      this.currentFieldObj[\"type\"] = eleType;\r\n      this.currentFieldObj[\"fieldName\"] = this.list2[index][\"label\"][\"default\"];\r\n\r\n      if(this.list2[index][\"required\"]){\r\n        this.currentFieldObj[\"fieldRequired\"] = this.list2[index][\"required\"];\r\n      }else{\r\n        this.currentFieldObj[\"fieldRequired\"] = false;\r\n      }\r\n\r\n      if(this.list2[index][\"maxLength\"]){\r\n        this.currentFieldObj[\"fieldMaxLength\"] = this.list2[index][\"maxLength\"];\r\n      }else{\r\n        this.currentFieldObj[\"fieldMaxLength\"] = null;\r\n      }\r\n\r\n      if(this.list2[index][\"minLength\"]){\r\n        this.currentFieldObj[\"fieldMinLength\"] = this.list2[index][\"minLength\"];\r\n      }else{\r\n        this.currentFieldObj[\"fieldMinLength\"] = null;\r\n      }\r\n\r\n      if(this.list2[index][\"maxValue\"]){\r\n        this.currentFieldObj[\"fieldMaxValue\"] = this.list2[index][\"maxValue\"];\r\n      }\r\n\r\n      if(this.list2[index][\"minValue\"]){\r\n        this.currentFieldObj[\"fieldMinValue\"] = this.list2[index][\"minValue\"];\r\n      }\r\n\r\n      if(this.list2[index][\"options\"]){\r\n        var optionToEditList = [];\r\n        for(var j in this.list2[index][\"options\"]){\r\n          optionToEditList.push(\r\n            {\r\n              \"rid\": this.list2[index][\"options\"][j][\"id\"],\r\n              \"rName\": this.list2[index][\"options\"][j][\"name\"],\r\n              \"rValue\": this.list2[index][\"options\"][j][\"value\"],\r\n              \"rLabel\": this.list2[index][\"options\"][j][\"label\"],\r\n              \"rChecked\": this.list2[index][\"options\"][j][\"checked\"],\r\n            });\r\n        }\r\n        this.currentFieldObj[\"fieldOptions\"] = optionToEditList;\r\n      }else{\r\n        this.currentFieldObj[\"fieldOptions\"] = null;\r\n      }\r\n\r\n      if(this.list2[index][\"fields\"]){\r\n        var dpToEditList = [];\r\n\r\n        for(var k in this.list2[index][\"fields\"]){\r\n          dpToEditList.push(\r\n            {\r\n              \"rid\": this.list2[index][\"fields\"][k][\"id\"],\r\n              \"rName\": this.list2[index][\"fields\"][k][\"name\"],\r\n              \"rType\": this.list2[index][\"fields\"][k][\"type\"],\r\n              \"rLabel\": this.list2[index][\"fields\"][k][\"label\"],\r\n              \"rRequired\": this.list2[index][\"fields\"][k][\"required\"],\r\n            });\r\n        }\r\n        this.currentFieldObj[\"fieldFields\"] = dpToEditList;\r\n      }else{\r\n        this.currentFieldObj[\"fieldFields\"] = null;\r\n      }\r\n\r\n      if(this.list2[index][\"config\"]){\r\n        this.currentFieldObj[\"fieldDateConfig\"] = this.list2[index][\"config\"];\r\n      }else{\r\n        this.currentFieldObj[\"fieldDateConfig\"] = null;\r\n      }\r\n\r\n      if(this.list2[index][\"html\"]){\r\n        this.currentFieldObj[\"fieldHtml\"] = this.list2[index][\"html\"];\r\n      }else{\r\n        this.currentFieldObj[\"fieldHtml\"] = null;\r\n      }\r\n      \r\n      this.fieldNameArray = [];\r\n      for(var i in this.list2){\r\n        this.fieldNameArray.push(this.list2[i][\"name\"].toLowerCase());\r\n      }\r\n      this.showEditModal = true;\r\n      this.changeNameIndex = index;\r\n      var fieldNum = parseInt(index)+1;\r\n      this.modalTitle = \"Edit Field (\"+fieldNum+\") - Type: \"+eleType;\r\n      this.modalContent = \"\";\r\n    },\r\n    closeEditModal(changesObj){\r\n\r\n      this.list2[this.changeNameIndex][\"name\"] = changesObj[\"fieldName\"];\r\n      this.list2[this.changeNameIndex][\"id\"] = changesObj[\"fieldId\"];\r\n      this.list2[this.changeNameIndex][\"label\"] = changesObj[\"fieldLabel\"];\r\n      this.list2[this.changeNameIndex][\"required\"] = changesObj[\"fieldRequired\"];\r\n\r\n      if(changesObj[\"fieldOptions\"]){\r\n        this.list2[this.changeNameIndex][\"options\"] = changesObj[\"fieldOptions\"];\r\n      }\r\n\r\n      if(changesObj[\"fieldFields\"]){\r\n        this.list2[this.changeNameIndex][\"fields\"] = changesObj[\"fieldFields\"];\r\n      }\r\n\r\n      if(changesObj[\"fieldMaxLength\"]){\r\n        this.list2[this.changeNameIndex][\"maxLength\"] = changesObj[\"fieldMaxLength\"];\r\n      }\r\n\r\n      if(changesObj[\"fieldMinLength\"]){\r\n        this.list2[this.changeNameIndex][\"minLength\"] = changesObj[\"fieldMinLength\"];\r\n      }\r\n\r\n      if(changesObj[\"fieldMaxValue\"]){\r\n        this.list2[this.changeNameIndex][\"maxValue\"] = changesObj[\"fieldMaxValue\"];\r\n      }\r\n\r\n      if(changesObj[\"fieldMinValue\"]){\r\n        this.list2[this.changeNameIndex][\"minValue\"] = changesObj[\"fieldMinValue\"];\r\n      }\r\n\r\n      if(changesObj[\"fieldHtml\"]){\r\n        this.list2[this.changeNameIndex][\"html\"] = changesObj[\"fieldHtml\"];\r\n      }\r\n\r\n      if(changesObj[\"fieldDateConfig\"]){\r\n        this.list2[this.changeNameIndex][\"config\"] = changesObj[\"fieldDateConfig\"];\r\n      }else{\r\n        if(this.list2[this.changeNameIndex][\"config\"]){\r\n          this.list2[this.changeNameIndex][\"config\"] = \"\";\r\n        }\r\n      }\r\n\r\n      this.$emit('returnFieldList', this.list2);\r\n      this.showEditModal = false;;\r\n      return false;\r\n    },\r\n    closeEditModalCancel(bool){\r\n      this.showEditModal = bool;\r\n      return bool;\r\n    },\r\n    getPlaceholder(type,index){ //in component maybe\r\n      var phText = \"\";\r\n      var formatType = type.substring(0,1).toUpperCase() + type.substring(1,type.length);\r\n      if(type=='date'){\r\n        formatType = \"YYYY/MM/DD\"\r\n      }\r\n\r\n      if(this.list2[index][\"required\"] && this.list2[index][\"required\"]==true){\r\n        phText = formatType +\" (Required)\";\r\n      }else{\r\n        phText = formatType;\r\n      }\r\n      return phText;\r\n    },\r\n  },\r\n  watch:{\r\n      list2(){\r\n          console.log(\"list change\",this.list2);\r\n          this.$emit('returnFieldList', this.list2);\r\n          if(this.list2.length > this.oldListLength ){\r\n            if(this.list2.length>0 && this.list2[this.list2.length-1].type==\"html\"){\r\n              this.editElementFromFieldList(this.list2.length-1 ,this.list2[this.list2.length-1].type);\r\n              this.oldListLength = this.list2.length-1;\r\n            }\r\n          }\r\n      }\r\n  }\r\n};\r\n</script>\r\n\r\n<template>\r\n  <div class=\"drag-row\">\r\n    <modal :show=\"showChangeNameModal\" :buttonName=\"'OK'\" :name=\"'changeNameModel'\" :fieldNameArray=\"fieldNameArray\" :title=\"modalTitle\" @closeModal=\"closeChangeNameModal\" @closeModalCancel=\"closeChangeNameModalCancel\">\r\n    </modal>\r\n    <modal :show=\"showEditModal\" :buttonName=\"'OK'\" :name=\"'editFieldModel'\" :fieldValue=\"currentFieldObj\" :fieldNameArray=\"fieldNameArray\" :title=\"modalTitle\" @closeModal=\"closeEditModal\" @closeModalCancel=\"closeEditModalCancel\">\r\n    </modal>\r\n    <div class=\"option-list\">\r\n      <h3>Types</h3>\r\n      <draggable\r\n        class=\"dragArea list-group\"\r\n        :list=\"list1\"\r\n        :group=\"{ name: 'people', pull: 'clone', put: false }\"\r\n        :sort=\"false\"\r\n        @change=\"log\"\r\n      >\r\n        <div\r\n          class=\"list-group-item\"\r\n          v-for=\"(element,index) in list1\"\r\n          :key=\"element.name+index\"\r\n          @click=\"addItemToList(index)\"\r\n        >\r\n          {{ element.label.default }}\r\n        </div>\r\n      </draggable>\r\n    </div>\r\n\r\n    <div class=\"field-list\">\r\n      <h3>Fields</h3>\r\n      <draggable\r\n        class=\"dragArea list-group\"\r\n        :list=\"list2\"\r\n        group=\"people\"\r\n        @change=\"log\"\r\n      >\r\n      <div v-show=\"list2.length>0\"\r\n            class=\"list-group-item\"\r\n            v-for=\"(element,index) in list2\"\r\n            :key=\"element.name+index\"\r\n            @mouseover=\"showCancelIndex = element\"\r\n            @mouseout=\"showCancelIndex = null\"\r\n            >\r\n            <div class=\"wrapper-table-create-form\">\r\n              <div class=\"item-upper-section\">\r\n                <span class=\"item-title\" @click=\"changeFieldName(index)\">\r\n                  {{index+1}}. {{ element.label.default }}\r\n\t\t\t          </span>\r\n                <span v-show=\"element.required && element.required==true && element.type!='paneldynamic'\">*</span>\r\n                <span v-show=\"element.type=='paneldynamic' && element.fields[0].required==true\">*</span>\r\n                <span class=\"item-edit\" v-show=\"showCancelIndex === element\" @click=\"editElementFromFieldList(index,element.type)\">\r\n                  Edit\r\n                </span>\r\n                \r\n                <span class=\"item-cancel\" v-show=\"showCancelIndex === element\" @click=\"removeElementFromFieldList(index)\">\r\n                  x\r\n                </span>\r\n              </div>\r\n\t\t\t        <div class=\"item-content\" v-if=\"element.type=='text'\">\r\n\t\t\t\t        <div class=\"wrapper-input\">\r\n                  <input type=\"text\" class=\"form-control\" placeholder=\"Text\" input-text readonly/>\r\n                </div>\r\n\t\t\t\t      </div>\r\n              <div class=\"item-content\" v-else-if=\"element.type=='number'\">\r\n\t\t\t\t        <div class=\"wrapper-input\">\r\n                  <input type=\"text\" class=\"form-control\" placeholder=\"Number\" input-text readonly/>\r\n                </div>\r\n\t\t\t\t      </div>\r\n              <div class=\"item-content\" v-else-if=\"element.type=='imageFile'\">\r\n\t\t\t\t        <div class=\"wrapper-input\">\r\n                  <button type=\"button\">\r\n                    <span>Upload Image</span>\r\n                  </button>\r\n                </div>\r\n\t\t\t\t      </div>\r\n              <div class=\"item-content\" v-else-if=\"element.type=='email'\">\r\n\t\t\t\t        <div class=\"wrapper-input\">\r\n                  <input type=\"email\" class=\"form-control\"  input-text placeholder=\"Email\" readonly/>\r\n                </div>\r\n\t\t\t\t       </div>\r\n              <div class=\"item-content\" v-else-if=\"element.type=='date'\">\r\n\t\t\t\t        <div class=\"wrapper-input\">\r\n                  <input type=\"text\" class=\"form-control\" placeholder=\"YYYY/MM/DD\" input-text readonly/>\r\n                </div>\r\n\t\t\t\t      </div>\r\n              <div class=\"item-content\" v-else-if=\"element.type=='textarea'\">\r\n\t\t\t\t        <div class=\"wrapper-input\">\r\n                  <textarea class=\"form-control intput-text-3\" rows=\"3\" placeholder=\"Textarea\" cols=\"25\" input-text readonly> \r\n\t\t\t\t          </textarea>\r\n                </div>\r\n\t\t\t\t      </div>\r\n              <div class=\"item-content-mobile\" v-else-if=\"element.type=='mobile'\">\r\n\t\t\t\t        <div class=\"wrapper-input-cc\">\r\n\t\t\t\t\t        <input type=\"tel\" class=\"form-control\" input-text  string-to-number autocomplete=\"off\" placeholder=\"Country Code\" readonly/>  \r\n\t\t\t\t        </div>\r\n\t\t\t\t        <div class=\"wrapper-input-number\">\r\n\t\t\t\t\t        <input type=\"tel\" class=\"form-control\" input-text  string-to-number autocomplete=\"off\" placeholder=\"Number\" readonly/>\r\n\t\t\t\t\t\r\n\t\t\t\t        </div>\r\n\t\t\t        </div>\r\n              <div class=\"item-content\" v-else-if=\"element.type=='radio'\">\r\n\t\t\t\t        <label class=\"form-control\" v-for=\"(option,indexRadio) in element.options\" :key=\"indexRadio\">\r\n\t\t\t\t          <input v-bind:id=\"option.id\" class=\"radio-custom\" type=\"radio\" disabled>\r\n\t\t\t\t\t        <label v-bind:for=\"option.id\">{{ option.label.default }}</label>\r\n\t\t\t\t        </label>\r\n\t\t\t        </div>\r\n              <div class=\"item-content\" v-else-if=\"element.type=='multipleCheckboxes'\">\r\n\t\t\t\t        <label class=\"form-control\" v-for=\"(option,indexCheckbox) in element.options\" :key=\"indexCheckbox\">\r\n\t\t\t\t            <input v-bind:id=\"option.id\" class=\"checkbox-custom\" type=\"checkbox\" disabled>\r\n\t\t\t\t\t          <label v-bind:for=\"option.id\" class=\"checkbox-custom-label\">{{ option.label.default }}</label>\r\n\t\t\t\t        </label>\r\n              </div>\r\n              <div class=\"item-content\" v-else-if=\"element.type=='singleCheckbox'\">\r\n\t\t\t\t          <input class=\"checkbox-custom\" type=\"checkbox\" disabled/>\r\n                  <label class=\"checkbox-custom-label\">&nbsp;</label>\r\n\t\t\t        </div>\r\n              <div class=\"item-content\" v-else-if=\"element.type=='select'\">\r\n                <select class=\"form-control\">\r\n                  <option value=\"\">Please select one</option>\r\n                  <option v-for=\"(option,indexSelect) in element.options\" :key=\"indexSelect\">{{option.label.default}}</option>\r\n                </select>\r\n              </div> \r\n              <div class=\"item-content-panel-dynamic\" v-else-if=\"element.type=='paneldynamic'\">\r\n                <button @click=\"addFieldInPanel(index)\">Add</button>\r\n                <div class=\"input-field\" v-for=\"(option,indexPd) in element.fields\" :key=\"indexPd\">\r\n                  <input v-bind:id=\"option.id\" v-bind:type=\"option.type\" class=\"form-control\" placeholder=\"Text\" input-text readonly/>\r\n                </div>\r\n                <button v-show=\"element.fields.length>1\" @click=\"deleteFieldInPanel(index)\">Delete</button>\r\n              </div> \r\n              <div class=\"item-content\" v-else-if=\"element.type=='html'\">\r\n\t\t\t\t        <div class=\"wrapper-input\">\r\n                  <!--<vue-editor placeholder=\"Insert Image / Enter text for your promotion page\" v-model=\"element.html\"></vue-editor>-->\r\n                  <div v-if=\"element.html\" v-html=\"element.html\"></div>\r\n                  <div v-else>Html element must not be emptied!</div>\r\n                  \r\n                </div>\r\n\t\t\t\t      </div>\r\n              <div class=\"item-content\" v-else>\r\n\t\t\t\t        <div class=\"wrapper-input\">\r\n                  not yet\r\n                </div>\r\n\t\t\t\t      </div>\r\n\t\t\t      </div>\r\n      </div>\r\n      <div v-show=\"list2.length==0\">Select types from the left panel to create a form!</div>\r\n      </draggable>\r\n    </div>\r\n  </div>\r\n</template>\r\n<style scoped></style>"]}]}